{
  "manifest_version": 2,
  "name": "MyNotes",
  "version": "1.0.0",
  "description": "NoteKeep: Your Personalized Note Storage Solution",
  "author": "Nitesh Nagar",
  "browser_action": {
    "default_icon": {
      "16": "logo_16x16.png",
      "48": "logo_48x48.png",
      "128": "logo_128x128.png"
    },
    "default_popup": "index.html",
    "browser_style": true
  },
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "nitesh9109@duck.com",
      "strict_min_version": "88.0"
    }
  }
}
